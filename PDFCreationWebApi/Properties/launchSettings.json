{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5290"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionStrings__Default": "Server=ARTPC;Database=PDFTemplates;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7156;http://localhost:5290"
